<div class="absolute top-0 z-50 w-full">
<div class="section container">
  <div class="branding text-green-light">
    <a href="{{ site.link }}">
      {% include "components/header-logo.svg.twig" %}
    </a>
  </div>
</div>
</div>

<header class="header-main header-unpin z-50">
  <div class="container">

    {% block header %}

      <nav id="nav-main" class="nav-main flex justify-end items-center" role="navigation">


        <div class="nav-menu text-hero leading-none z-40">
        <div class="container">
          <div class="row">
            <div class="col h-screen ml-2/12">
              {% include "components/menu.html.twig" with {
                "menu" : menu.get_items
              } %}
            </div>
          </div>
        </div>
        </div>

        <div class="nav-burger toggle-menu relative z-50">
          <span></span>
          <span></span>
          <span></span>
        </div>

      </nav>

    {% endblock %}

  </div>
</header>
